<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwipeFlingAdapterView">
        <attr name="SwipeFlingStyle" format="reference"/>
        <attr name="rotation_degrees" format="float"/>
        <attr name="min_adapter_stack" format="integer"/>
        <attr name="max_visible" format="integer"/>
    </declare-styleable>

    <declare-styleable name="NiceSpinner">
        <attr name="arrowTint" format="color" />
        <attr name="hideArrow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomTextView">
        <attr name="textViewFontName" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="editTextFontName" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="buttonFontName" format="string" />
    </declare-styleable>

</resources>